{
  "name": "ui-boilerplate",
  "version": "1.0.0",
  "description": "NodeJS, Express, SSR, React, React Router, Redux, Redux Saga, SASS, Jest, Webpack, Babel, ESLint, Web App Boilerplate",
  "main": "src/index.js",
  "repository": "https://github.com/duytan85/boilerplate",
  "author": "Duytan Hoang <duytanhoang85@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3",
    "yarn": ">=1.15.2"
  },
  "scripts": {
    "---------- RUN ----------": "",
    "start": "node dist/server.js",
    "start:dev": "NODE_ENV=development concurrently 'yarn start-dev-client' 'yarn start-dev-server'",
    "start-dev-server": "nodemon --exec babel-node --watch src src/index.js",
    "start-dev-client": "webpack-dev-server --config config/webpack/webpack.config.client.js",
    "---------- BUILD ----------": "",
    "build": "node scripts/build.js",
    "---------- TEST ----------": "",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage",
    "---------- LINT ----------": "",
    "lint": "eslint --ext .js --ext .jsx src config",
    "lint:fix": "eslint --ext .js --ext .jsx src config --fix",
    "format": "prettier --write -- '**/*.js'"
  },
  "_moduleAliases": {
    "@": "."
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "axios": "0.19.0",
    "dotenv": "8.0.0",
    "express": "4.17.3",
    "express-ping": "1.4.0",
    "history": "4.9.0",
    "http-proxy-middleware": "0.19.1",
    "lodash.clonedeep": "4.5.0",
    "module-alias": "2.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "assets-webpack-plugin": "3.9.10",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "clean-webpack-plugin": "2.0.2",
    "concurrently": "4.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-react": "7.13.0",
    "jest": "24.8.0",
    "nodemon": "1.19.0",
    "prettier": "1.18.2",
    "webpack": "4.32.2",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1",
    "webpack-node-externals": "1.7.2"
  }
}
